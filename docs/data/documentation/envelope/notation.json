":"text","text":": "},{"kind":"typeIdentifier","text":"Bool","preciseIdentifier":"s:Sb"},{"kind":"text","text":", "},{"kind":"externalParam","text":"highlighting"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"Set","preciseIdentifier":"s:Sh"},{"kind":"text","text":"<"},{"kind":"typeIdentifier","text":"Digest","preciseIdentifier":"s:16SecureComponents6DigestV"},{"kind":"text","text":">) -> "},{"kind":"typeIdentifier","text":"String","preciseIdentifier":"s:SS"}],"abstract":[{"type":"text","text":"Returns the tree notation for this envelope."}],"identifier":"doc:\/\/Envelope\/documentation\/Envelope\/Envelope\/treeFormat(hideNodes:highlighting:)","kind":"symbol","type":"topic","url":"\/documentation\/envelope\/envelope\/treeformat(hidenodes:highlighting:)"},"doc://Envelope/documentation/Envelope/Elision":{"role":"article","title":"Elision and Redaction","abstract":[{"type":"text","text":"Removing information without invalidating the digest tree."}],"identifier":"doc:\/\/Envelope\/documentation\/Envelope\/Elision","kind":"article","type":"topic","url":"\/documentation\/envelope\/elision"},"doc://Envelope/documentation/Envelope/OutputFormats":{"role":"article","title":"Output Formats","abstract":[{"type":"text","text":"A comparison of the various Envelope output formats."}],"identifier":"doc:\/\/Envelope\/documentation\/Envelope\/OutputFormats","kind":"article","type":"topic","url":"\/documentation\/envelope\/outputformats"}}}